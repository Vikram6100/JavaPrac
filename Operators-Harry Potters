import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int n=Math.abs(a);
        int f=n/1000;
        int l =n%10;
        int add=f+l;
        System.out.print(add);
        
    }
}
