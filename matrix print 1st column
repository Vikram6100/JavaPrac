import java.io.*;
import java.util.*;

class HelloWorld {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int ans=0;
         int arr[][]=new int [n][n];
         for(int i=0;i<n;i++){
             for(int j=0;j<n;j++){
                 arr[i][j]=scanner.nextInt();
             }
         }
        for(int i=0;i<n;i++){
               ans+=arr[i][0];
        }
        System.out.print(ans);
    }
}
