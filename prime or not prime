import java.io.*;
import java.util.*;

class vik
{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int prime=1;
        for(int i=2;i<n;i++){
            if(n %i == 0){
            prime=0;
            break;
            
        }
        
       
    }if(prime == 1){
        System.out.print("Prime Number");
    }
    else{
        System.out.print("Not A Prime Number ");
    }
    
}}
